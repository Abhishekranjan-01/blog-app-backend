{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/users",
      "dest": "/api/users.js"
    },
    {
      "src": "/api/blogs",
      "dest": "/api/blogs.js"
    },
    {
      "src": "/",
      "dest": "/api/index.js"
    }
  ]
}
